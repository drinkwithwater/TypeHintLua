{
	"name": "TypeHintLua",
	"description": "TypeHintLua extension",
	"license": "MIT",
	"version": "0.5.6",
	"repository": {
		"type": "git",
		"url": "https://github.com/drinkwithwater/TypeHintLua/"
	},
	"author": "drinkwithwater",
	"publisher": "drinkwithwater",
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"TypeHintLua"
	],
	"engines": {
		"vscode": "^1.74.0"
	},
	"activationEvents": [
		"onLanguage:thlua"
	],
	"main": "./client/out/extension",
	"contributes": {
		"languages": [
			{
				"id": "thlua",
				"aliases": [
					"TypeHintLua",
					"thlua"
				],
				"extensions": [
					".thlua"
				],
				"configuration": "./syntax/language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "thlua",
				"scopeName": "source.thlua",
				"path": "./syntax/thlua.tmLanguage.json"
			}
		],
		"configuration": {
			"type": "object",
			"title": "TypeHintLua",
			"properties": {
			}
		},
		"commands":[{
			"command":"TypeHintLua.runCode",
			"title":"TypeHintLua: Run"
		}],
		"menus":{
			"editor/context":[
				{
					"command":"TypeHintLua.runCode",
					"group":"TypeHintLua",
					"when":"resourceLangId == 'thlua'"
				}
			]
		}
	},
	"devDependencies": {
		"@types/node": "^18.15.3"
	}
}
