#!/usr/bin/lua

local function getcontents (filename)
  local file = assert(io.open(filename, "r"))
  local contents = file:read("*a")
  file:close()
  return contents
end

local filename = arg[1]
if not filename then
  print("usage : the xxx.thlua")
end

local subject = getcontents(filename)

local thlua = require "thlua"
thlua:main(filename, subject)

-- print("============later call============================")
thlua:checkDefineLuaFunction()
--thlua:checkLateLuaFunction()

