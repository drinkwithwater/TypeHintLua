
-- define a const variable
const constHello = "hello world"

-- define a local value, literal type auto cast to primitive type in local statement
local localBoolean = true
localBoolean = false

-- print in hint space
(@print($constHello))

-- print in lua space
print(constHello)

-- hint at symbol
const i:Integer = 321

-- type cast
const i = 321 @ Integer

