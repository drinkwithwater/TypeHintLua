
-- define a function with multi return type
const function multiReturn(a:Integer):Ret(True, Integer):Ret(False, String)
	if math.random() > 0.5 then
		return true, a
	else
		return false, "fdsfsd"
	end
end

const ok, data = multiReturn(321321)

if ok then
	(@print($data))
else
	(@print($data))
end
