
const TestCase = require "test.TestCase"

TestCase.expect([[
const function dosth(a)
end
dosth(321)
]], {1})

TestCase.expect([[
const function yes() end
const function dosth(...)
	yes(...)
end
dosth(321321)
dosth()
]], {5})


TestCase.expect([[
const function yes(a:Integer) end
const function dosth(...)
	yes(...)
end
dosth("fdsfs")
dosth(321)
]], {5})

TestCase.expect([[
const function yes(a:Integer, b:String) end
const function dosth(...)
	yes(...)
end
dosth("fdsfs", "fdsfs")
dosth(321, "fdsfsf")
]], {5})

TestCase.expect([[
const function yes(a:Integer, b:String) end
const function dosth(...)
	yes(321, ...)
end
dosth(321, "fdsfs")
dosth("fdsfs")
]], {5})

TestCase.expect([[
const function yes(a:Integer, b:String) end
const function dosth(...)
	yes(..., "fdsfs")
end
dosth(321)
]], {2})

TestCase.expect([[
const function yes(a:Integer, b:String) end
const function dosth(...)
	yes(..., "fdsfs")
	yes(...)
end
dosth(321)
]], {6})
