
(@do

let.class = import("thlua.type.TypeClass").class

end)

local Exception = {}

function Exception.new(vMsg:String, vNode:Union(class.AstNode, False, Nil))::RetClass(class.Exception)
	return {::Self()
		msg=vMsg,
		node=vNode or nil,
	}
end

function Exception:fixNode(vNode:class.AstNode)::nocheck()
end

function Exception.is(v)::isguard(class.Exception)
	return false
end

return Exception
