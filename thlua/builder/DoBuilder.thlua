
const Exception = require "thlua.Exception"
(@let.class = import("thlua.type.TypeClass").class)

const DoBuilder = {}
DoBuilder.__index=DoBuilder

function.class DoBuilder.new(vContext:class.Context, vNode:class.IAstNode):RetClass(let.DoBuilder)
	return setmetatable({.class
		pass=false@Boolean,
	}, DoBuilder)
end

function DoBuilder:build(vHintInfo:class.TLongHintInfo(Nil)):Ret()
	if vHintInfo.attrSet.pass then
		self.pass = true
	end
end

return DoBuilder
