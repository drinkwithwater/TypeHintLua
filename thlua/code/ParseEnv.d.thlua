
(@let.node = import("thlua.code.Node").node)

local ParseEnv = {}

function.nocheck ParseEnv.parse(vContent:String):Ret(node.Chunk, String):Ret(False, node.Error)
end

function.nocheck ParseEnv.compile(vChunk:String, vChunkName:OrNil(String)):Ret(String):Ret(False, String)
end

function.nocheck ParseEnv.load(vChunk:String, vChunkName:OrNil(String), vMode:OrNil(String), vEnvTable:OrNil(Truth)):Ret(AnyFunction):Ret(False, String)
end

return ParseEnv
