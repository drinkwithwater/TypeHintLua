
(@let.node = import("thlua.code.Node").node)

local ParseEnv = {}
ParseEnv.__index=ParseEnv

function:class(let.ParseEnv) ParseEnv.new(vSubject:String, vChunkName:String)
	return setmetatable({.class}, ParseEnv)
end

function.pass ParseEnv:get():Ret(Union(node.Error, node.Chunk))
end

function.pass ParseEnv.parse(vContent:String):Ret(node.Chunk):Ret(False, node.Error)
end

return ParseEnv
