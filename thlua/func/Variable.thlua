
const Variable = {}

(@let.class = import("thlua.type.TypeClass").class)

Variable.__index=Variable

function.class Variable.new(vManager:class.TypeManager, vIsSelf:Boolean):RetClass(class.Variable)
	const self = setmetatable({.class
		_manager=vManager,
		_isSelf=vIsSelf,
	}, Variable)
	return self
end

function.open Variable.is(t):RetIs(class.Variable)
	return getmetatable(t) == Variable
end

return Variable
