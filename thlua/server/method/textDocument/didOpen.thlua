
(@do
    var.lsp=import("thlua.server.protocol").lsp

    var.DidOpenTextDocumentParams = Struct {
        textDocument = Struct {
            uri=String,
            version=Number,
            text=String,
        }
    }

end)

return function(vClient:lsp.Client, vParams)::Ret()
    vClient.runtime:main(vParams.textDocument.uri, vParams.textDocument.text)
end
