
const Node = require "thlua.code.Node"
const class = require "thlua.class"

(@let.clazz = import($class).clazz)

const BaseSpaceCom = class@<clazz.BaseSpaceCom, false>()

function.open BaseSpaceCom:ctor(vManager:clazz.TypeManager, vNode:clazz.IBaseNode, ...)
    self._manager = vManager
    self._node = vNode
    self._refer = false @ OrFalse(clazz.Reference)
end

function BaseSpaceCom:setRefer(vRefer:clazz.Reference)
    self._refer = vRefer
end

function BaseSpaceCom:getRefer():Ret(OrFalse(clazz.Reference))
    return self._refer
end

function BaseSpaceCom:getNode()
    return self._node
end

return BaseSpaceCom
