
const AutoTerm = {}
AutoTerm.__index=AutoTerm
AutoTerm.__tostring=function(self)
	return "auto-"..tostring(next(self._symbolSet))
end

(@let.clazz = import("thlua.type.TypeClass").clazz)

function:class(clazz.AutoTerm) AutoTerm.new(vManager:clazz.TypeManager, vRefer:clazz.Reference)
	return setmetatable({.class
		_manager=vManager,
		_refer=vRefer,
	}, AutoTerm)
end

return AutoTerm
