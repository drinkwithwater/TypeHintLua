
(@let.StringToString = Dict (String, String))

local comparison:StringToString = {
	[">"]="__lt",
	["<"]="__lt",
	[">="]="__le",
	["<="]="__le",
}

local mathematic:StringToString = {
	["+"]="__add",
	["-"]="__sub",
	["*"]="__mul",
	["/"]="__div",
	["//"]="__idiv",
	["%"]="__mod",
	["^"]="__pow",
}

local bitwise:StringToString = {
	["&"]="__band",
	["|"]="__bor",
	["~"]="__bxor",
	["<<"]="__shr",
	[">>"]="__shl",
}

local all:StringToString = {
	[".."]="__concat"
}

for k,v in pairs(comparison) do
	all[k] = v
end

for k,v in pairs(bitwise) do
	all[k] = v
end

for k,v in pairs(mathematic) do
	all[k] = v
end

return {
	bitwise=bitwise,
	mathematic=mathematic,
	comparison=comparison,
	all=all,
}
